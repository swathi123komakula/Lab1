<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="sbu" class="com.cg.demo1.c.SBU">
	<property name="sbuCode" value="600"></property>
	<property name="sbuName" value="Product Engineering Services"></property>
	<property name="sbuHead" value="Bhanu"></property>
	<property name="emplist" >
	<list>
	<bean class="com.cg.demo1.c.Employee">
	<property name="employeeId" value="15"></property>
	<property name="employeeName" value="Swathi"></property>
	<property name="salary" value="30000"></property>
	</bean>
	<bean class="com.cg.demo1.c.Employee">
	<property name="employeeId" value="16"></property>
	<property name="employeeName" value="Nandini"></property>
	<property name="salary" value="32000"></property>
	</bean>
	</list>
	</property>
	</bean>

</beans>
